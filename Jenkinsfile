pipeline {
    agent any
       
       stages {
            stage('Compile'){
               steps{
                   script{
                       echo'Compile the code'
                   }
               }
           }
           stage('Unittest'){
               steps{
                   script{
                       echo'Unittest the code'
                   }
               }
           }
           
           stage('Package'){
               steps{
                   script{
                       echo'Package the code'
                   }
               }
           }
           
           
       }
}
